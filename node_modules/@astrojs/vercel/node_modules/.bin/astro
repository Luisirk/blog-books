#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/luisi/blog-books/node_modules/.pnpm/astro@5.3.1_jiti@2.4.2_ligh_0071d12fd0f09ef4854b0bd69339054b/node_modules/astro/node_modules:/mnt/c/Users/luisi/blog-books/node_modules/.pnpm/astro@5.3.1_jiti@2.4.2_ligh_0071d12fd0f09ef4854b0bd69339054b/node_modules:/mnt/c/Users/luisi/blog-books/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/luisi/blog-books/node_modules/.pnpm/astro@5.3.1_jiti@2.4.2_ligh_0071d12fd0f09ef4854b0bd69339054b/node_modules/astro/node_modules:/mnt/c/Users/luisi/blog-books/node_modules/.pnpm/astro@5.3.1_jiti@2.4.2_ligh_0071d12fd0f09ef4854b0bd69339054b/node_modules:/mnt/c/Users/luisi/blog-books/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.3.1_jiti@2.4.2_ligh_0071d12fd0f09ef4854b0bd69339054b/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.3.1_jiti@2.4.2_ligh_0071d12fd0f09ef4854b0bd69339054b/node_modules/astro/astro.js" "$@"
fi
